{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-new": "off",
        "max-depth": ["error", 2],
        "max-lines-per-function": ["error", 15],
        "no-alert": "off",
        "no-console": "off",
        "import/extensions": ["off"]
    }
}
